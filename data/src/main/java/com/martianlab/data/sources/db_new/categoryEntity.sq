CREATE TABLE IF NOT EXISTS `category` (
    `id` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    `imageUrl` TEXT,
    `title` TEXT NOT NULL,
    `sort` INTEGER as Int NOT NULL,
    `total` INTEGER as Int NOT NULL
);

getAll:
SELECT *
FROM category;


getById:
SELECT *
FROM category
WHERE id = ?;

insert:
INSERT INTO category
VALUES ?;

insertAll:
INSERT INTO category
VALUES ?;

updateCategory:
INSERT OR REPLACE INTO category
VALUES ?;